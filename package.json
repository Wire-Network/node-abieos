{
  "name": "@eosrio/node-abieos",
  "version": "3.2.1",
  "description": "Node Bindings for abieos: Binary <> JSON conversion using ABIs.",
  "main": "./lib/abieos.js",
  "typings": "./lib/abieos.d.ts",
  "scripts": {
    "install": "node -e 'process.exit(0)'",
    "test": "cd examples && node basic.js",
    "tsc:watch": "tsc --watch",
    "clean": "cmake-js clean",
    "build:tsc": "tsc",
    "build:linux": "cmake-js compile --cc /usr/bin/clang-18 --cxx /usr/bin/clang++-18 && cp build/Release/node_abieos.node dist/abieos.node",
    "build:linux:ci": "cmake-js compile && cp build/Release/node_abieos.node dist/abieos.node",
    "build:win": "cmake-js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eosrio/node-abieos.git"
  },
  "author": "EOS Rio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eosrio/node-abieos/issues"
  },
  "homepage": "https://github.com/eosrio/node-abieos#readme",
  "devDependencies": {
    "@types/node": "20.12.7",
    "cmake-js": "7.3.0",
    "node-addon-api": "8.0.0",
    "typescript": "5.4.5"
  },
  "binary": {
    "napi_versions": [9]
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "20.0.0",
    "arch": "x64"
  }
}